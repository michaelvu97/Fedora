class HitBox
{
  private int x;
  private int y;
  private int height;
  private int width;
  
  public HitBox(int x, int y, int height, int width)
  {
    this.x=x;
    this.y=y;
    this.height=height;
    this.width=width;
  }
  public static boolean checkCollision(Hitbox a, Hitbox b)
  {
    int dx = a.x-b.x;
    int dy = a.y-b.y;
    if(Math.sqrt(dx*dx+dy*dy)<=)
    {
      
    }
  }
}
